{
  "name": "ldd3",
  "version": "0.0.13",
  "description": "Sets of (hopefully) easy to use charts designed with d3.",
  "main": "index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "prepublish": "npm run clean && tsc",
    "build-doc": "./node_modules/.bin/typedoc --exclude **/node_modules/** . --out docs --ignoreCompilerErrors",
    "build-doc2": "./node_modules/.bin/typedoc --mode modules --module commonjs --target es6 --json docs.json --excludePrivate",
    "start": "tsc -w",
    "clean": "rimraf src/**/*.js src/**/*.js.map src/**/*.d.ts",
    "build": "npm run clean && tsc && rollup -c"
  },
  "keywords": [
    "d3",
    "charts",
    "dataviz"
  ],
  "author": {
    "name": "Hugues Stefanski",
    "url": "https://wordpress.com/hstefanski"
  },
  "homepage": "https://github.com/Ledragon/ldd3",
  "repository": "https://github.com/Ledragon/ldd3",
  "license": "MIT",
  "devDependencies": {
    "@types/d3": "^4.4.1",
    "rimraf": "^2.6.0",
    "rollup": "^0.41.4",
    "rollup-plugin-node-resolve": "^2.0.0",
    "typedoc": "^0.5.5",
    "typedoc-markdown-theme": "0.0.4",
    "typescript": "^2.1.5"
  },
  "dependencies": {
    "d3": "^4.5.0"
  }
}
